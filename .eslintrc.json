{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "no-const-assign": "warn",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-continue": "off",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "constructor-super": "warn",
    "valid-typeof": "warn",
    "quotes": [0],
    "react/prefer-stateless-function": 0,
    "import/extensions": [
      1,
      "never"
    ],
    "prefer-destructuring": [1],
    "no-use-before-define": 0,
    "jsx-a11y/alt-text": 0,
    "comma-dangle": 0,
    "max-len": 0,
    "import/prefer-default-export": 0,
    "linebreak-style": 0,
    "react/forbid-prop-types": 0
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ]
}
